@model WebUI.Models.TelefoneViewModel

<form asp-action="Create">
    <input asp-for="ContatoId" hidden>
    <div class="form-group">
        <label asp-for="Numero" class="control-label">@Html.DisplayNameFor(model => model.Numero)</label>
        <input asp-for="Numero" class="form-control"/>
    </div>
    <div class="form-group">
        <input type="submit" class="btn btn-primary"/>
    </div>
</form>